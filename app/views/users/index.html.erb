<h1>Users List</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Relationship</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= relationship(user) %></td>
      <td><%= action(user) %></td>
    </tr>
  <% end %>
  </tbody>
</table>
